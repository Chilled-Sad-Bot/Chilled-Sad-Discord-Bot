{
  "name": "chilled-sad-discord-bot",
  "version": "1.0.0",
  "description": "A Discord Bot written in Node.js",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",

    "pm2": "pm2 start index.js --watch && pm2 monit",
    "pm2prod": "pm2 start index.js && pm2 monit",

    "pm2restart": "pm2 stop 0 && pm2 start index.js --watch && pm2 monit",
    "pm2restartprod": "pm2 stop 0 && pm2 start index.js && pm2 monit",

    "kill": "pm2 stop 0 && pm2 delete 0",

    "deployCommands": "node src/deploy-commands.js",
    
    "deployTables": "node _scripts/addTables.js"
  },
  "author": "BlackDayz",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^0.10.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "axios": "^0.24.0",
    "discord-api-types": "^0.26.0",
    "discord.js": "^13.4.0",
    "ms": "^2.1.3",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
    "rss-parser": "^3.12.0",
    "simple-node-logger": "^21.8.12",
    "simple-youtube-api": "^5.2.1"
  }
}
