{
  "name": "chilled-sad-discord-bot",
  "version": "0.49.7",
  "description": "A Discord Bot written in Node.js",
  "main": "shard.js",
  "scripts": {
    "dev": "nodemon index.js",
    "pm2dev": "pm2 start shard.js --name DEV__Mittelbot__DEV  && pm2 logs",
    "pm2devstop": "pm2 stop DEV__Mittelbot__DEV",
    "pm2devrestart": "pm2 stop DEV__Mittelbot__DEV && pm2 start shard.js --name DEV__Mittelbot__DEV  && pm2 logs",
    "pm2": "pm2 start shard.js --watch --name Mittelbot  && pm2 logs",
    "pm2prod": "pm2 start shard.js --name Mittelbot  && pm2 logs",
    "pm2restart": "pm2 stop Mittelbot && pm2 delete Mittelbot && pm2 start shard.js --watch --name Mittelbot  && pm2 logs",
    "pm2restartprod": "pm2 stop Mittelbot && pm2 delete Mittelbot && pm2 start shard.js --name Mittelbot  && pm2 logs",
    "kill": "pm2 stop Mittelbot && pm2 delete Mittelbot",
    "deployCommands": "node src/deploy-commands.js",
    "checkDatabase": "node _scripts/checkDatabase.js",
    "insertActiveCommands": "node _scripts/active_commands/insertCommandsInDatabase.js",
    "updateActiveCommands": "node _scripts/active_commands/updateCommandsInDatabase.js",
    "install_bot": "mkdir _logs && npm i",
    "transformDB": "node _scripts/transformData.js"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  },
  "author": "BlackDayz",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "@gonetone/get-youtube-id-by-url": "^1.0.0",
    "@napi-rs/canvas": "^0.1.29",
    "@twurple/api": "^5.2.3",
    "@twurple/auth": "^5.2.3",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "canvacord": "5.4.6",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "discord-bettermarkdown": "^1.2.0",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "extract-urls": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "memorystore": "^1.6.6",
    "ms": "^2.1.3",
    "mysql": "^2.18.1",
    "nconf": "^0.12.0",
    "node-notifier": "^10.0.1",
    "node-sloc": "^0.2.1",
    "nodemailer": "^6.7.8",
    "os-utils": "^0.0.14",
    "passport": "^0.6.0",
    "path": "^0.12.7",
    "random-animals-api": "^1.2.6",
    "rss-parser": "^3.12.0",
    "simple-node-logger": "^21.8.12",
    "simple-youtube-api": "^5.2.1",
    "sloc": "0.2.1",
    "translatte": "^3.0.1",
    "underscore": "^1.13.4",
    "undici": "^5.10.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "ytdl-core": "4.10.1"
  },
  "devDependencies": {
    "chalk": "^4.1.0",
    "nodemon": "^2.0.19",
    "string-width": "4.2.3"
  }
}
