{
    "name": "mittelbot",
    "version": "0.60.2",
    "description": "A Discord Bot written in Node.js",
    "main": "bot/core/shard.js",
    "private": true,
    "scripts": {
        "start": "docker compose up -d && docker logs -f mittelbot",
        "stop": "docker compose down",
        "restart": "npm run stop && npm run start",
        "log": "docker logs -f mittelbot",
        "format": "docker run --rm -v $(pwd):/app -w /app node:18.8 sh -c \"npm install -g prettier && prettier --write .\"",
        "check": "docker run --rm -v $(pwd):/app -w /app node:18.8 sh -c \"npm install -g prettier && prettier --check .\""
    },
    "nodemonConfig": {
        "ignore": [
            "*.json"
        ]
    },
    "author": "BlackDayz",
    "license": "GPL-3.0",
    "dependencies": {
        "@discord-player/equalizer": "^0.2.1",
        "@discord-player/extractor": "^4.1.2",
        "@discord-player/utils": "^0.2.1",
        "@discordjs/opus": "github:discordjs/opus",
        "@discordjs/rest": "^1.5.0",
        "@discordjs/voice": "^0.15.0",
        "@gonetone/get-youtube-id-by-url": "^1.0.0",
        "@napi-rs/canvas": "^0.1.39",
        "@twurple/api": "^6.0.9",
        "@twurple/auth": "^6.0.2",
        "axios": "^1.3.4",
        "body-parser": "^1.20.2",
        "caller-id": "^0.1.0",
        "canvacord": "5.4.8",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "discord-html-transcripts": "^3.1.4",
        "discord-logs": "^2.2.1",
        "discord-player": "^6.1.1",
        "discord.js": "^14.9.0",
        "dotenv": "^16.0.3",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-flash": "^0.0.2",
        "express-session": "^1.17.3",
        "express-validator": "^6.15.0",
        "extends-classes": "^1.0.5",
        "extract-urls": "^1.3.2",
        "ffmpeg-static": "^5.1.0",
        "jsonwebtoken": "^9.0.0",
        "memorystore": "^1.6.6",
        "ms": "^2.1.3",
        "mysql2": "^3.2.0",
        "nconf": "^0.12.0",
        "node-notifier": "^10.0.1",
        "node-sloc": "^0.2.1",
        "nodemailer": "^6.9.1",
        "os-utils": "^0.0.14",
        "passport": "^0.6.0",
        "random-animals-api": "^1.2.6",
        "rss-parser": "^3.12.0",
        "sequelize": "^6.30.0",
        "simple-node-logger": "^21.8.12",
        "simple-youtube-api": "^5.2.1",
        "translatte": "^3.0.1",
        "underscore": "^1.13.6",
        "undici": "^5.21.0",
        "uuid": "^9.0.0",
        "validator": "^13.9.0",
        "ytdl-core": "^4.11.2"
    },
    "devDependencies": {
        "chalk": "^4.1.0",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.7",
        "string-width": "4.2.3"
    }
}
