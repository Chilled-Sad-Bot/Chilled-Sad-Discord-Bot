{
    "name": "mittelbot",
    "version": "0.64.2",
    "description": "A Discord Bot written in Node.js",
    "main": "bot/core/shard.js",
    "private": true,
    "engines": {
        "node": ">=18.15.0"
    },
    "scripts": {
        "start": "docker compose up -d && docker logs -f mittelbot",
        "start-prod": "docker compose up -d",
        "stop": "docker compose down",
        "restart": "npm run stop && npm run start",
        "restart-prod": "npm run stop && npm run start-prod",
        "log": "docker logs -f mittelbot",
        "format": "npm run stop && docker run --rm -v $(pwd):/app -w /app node:18.8 sh -c \"npm install -g prettier && prettier --write .\"",
        "check": "docker run --rm -v $(pwd):/app -w /app node:18.8 sh -c \"npm install -g prettier && prettier --check .\"",
        "postinstall": "patch-package"
    },
    "nodemonConfig": {
        "ignore": [
            "*.json"
        ]
    },
    "author": "BlackDayz",
    "license": "GPL-3.0",
    "dependencies": {
        "@discord-player/equalizer": "^0.2.1",
        "@discord-player/extractor": "^4.2.1",
        "@discord-player/utils": "^0.2.1",
        "@discordjs/opus": "github:discordjs/opus",
        "@discordjs/rest": "^1.7.1",
        "@discordjs/voice": "^0.16.0",
        "@distube/ytdl-core": "^4.11.9",
        "@ffmpeg-installer/ffmpeg": "^1.1.0",
        "@gonetone/get-youtube-id-by-url": "^1.0.0",
        "@napi-rs/canvas": "^0.1.40",
        "@sentry/node": "^7.51.2",
        "@stdlib/assert-is-uri": "^0.0.8",
        "@twurple/api": "^6.2.0",
        "@twurple/auth": "^6.0.2",
        "axios": "^1.4.0",
        "body-parser": "^1.20.2",
        "caller-id": "^0.1.0",
        "canvacord": "5.4.9",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "discord-html-transcripts": "^3.1.4",
        "discord-logs": "^2.2.1",
        "discord-player": "^6.3.0",
        "discord.js": "^14.11.0",
        "dotenv": "^16.0.3",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-flash": "^0.0.2",
        "express-session": "^1.17.3",
        "express-validator": "^7.0.1",
        "extends-classes": "^1.0.5",
        "extract-urls": "^1.3.4",
        "ffmpeg-static": "^5.1.0",
        "imgur": "^2.2.0",
        "jsonwebtoken": "^9.0.0",
        "memorystore": "^1.6.6",
        "moment-timezone": "^0.5.43",
        "ms": "^2.1.3",
        "mysql2": "^3.3.1",
        "nconf": "^0.12.0",
        "node-notifier": "^10.0.1",
        "node-sloc": "^0.2.1",
        "nodemailer": "^6.9.2",
        "os-utils": "^0.0.14",
        "passport": "^0.6.0",
        "patch-package": "^7.0.0",
        "play-dl": "^1.9.6",
        "random-animals-api": "^1.2.6",
        "rss-parser": "^3.13.0",
        "sequelize": "^6.31.1",
        "simple-node-logger": "^21.8.12",
        "simple-youtube-api": "^5.2.1",
        "string-mismatch": "^2.1.1",
        "translatte": "^3.0.1",
        "underscore": "^1.13.6",
        "undici": "^5.22.1",
        "uuid": "^9.0.0",
        "validator": "^13.9.0",
        "ytdl-core": "^4.11.4"
    },
    "devDependencies": {
